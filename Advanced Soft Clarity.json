{
  "name": "Advanced Soft Clarity",
  "dataColors": [
    "#4E79A7", "#F28E2B", "#E15759", "#76B7B2", "#59A14F", "#EDC948",
    "#B07AA1", "#FF9DA7", "#9C755F", "#BAB0AC"
  ],
  "background": "#FFFFFF",
  "foreground": "#1F2937",
  "tableAccent": "#4E79A7",
  "visualStyles": {
    "*": {
      "*": {
        "background": [
          {
            "color": {
              "solid": { "color": "#FAFAFC" }
            },
            "transparency": 0
          }
        ],
        "border": [
          {
            "color": {
              "solid": { "color": "#E5E7EB" }
            },
            "show": true,
            "radius": 8
          }
        ],
        "dropShadow": [
          {
            "position": "Outer",
            "show": true,
            "color": {
              "solid": { "color": "#CBD5E1" }
            }
          }
        ],
        "visualHeader": [
          {
            "background": {
              "solid": { "color": "#F1F5F9" }
            },
            "foreground": {
              "solid": { "color": "#1F2937" }
            },
            "border": {
              "solid": { "color": "#E5E7EB" }
            }
          }
        ],
        "visualTooltip": [
          {
            "titleFontColor": {
              "solid": { "color": "#1F2937" }
            },
            "valueFontColor": {
              "solid": { "color": "#374151" }
            },
            "background": {
              "solid": { "color": "#F9FAFB" }
            }
          }
        ]
      }
    }
  },
  "textClasses": {
    "label": {
      "fontFace": "Segoe UI",
      "color": "#374151",
      "fontSize": 11
    },
    "callout": {
      "fontFace": "Segoe UI Semibold",
      "color": "#1F2937",
      "fontSize": 10
    },
    "title": {
      "fontFace": "Segoe UI Bold",
      "color": "#111827",
      "fontSize": 12
    },
    "header": {
      "fontFace": "Segoe UI Semibold",
      "color": "#1F2937",
      "fontSize": 11
    }
  },
  "good": "#22C55E",
  "neutral": "#D1D5DB",
  "bad": "#EF4444",
  "minimum": "#EF4444",
  "center": "#FACC15",
  "maximum": "#22C55E"
}
